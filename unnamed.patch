+}
+.lot{
+    background-color: rgba(0, 0, 0, 0.15);
+    color: #ffffff;
+    width:1094px;
+    margin-top: 16px;
+    border: 1px solid #007bff;
+}
+div ::-webkit-scrollbar {
+    width: 6px;
+    height: 6px;
+    background-color: #F5F5F5;
+    border-radius: 3px;
+ }
